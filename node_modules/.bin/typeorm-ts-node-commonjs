#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/pejmanhaghighatnia/Documents/AGI/node_modules/.pnpm/typeorm@0.3.25_pg@8.16.3_redis@4.7.1_reflect-metadata@0.1.14/node_modules/typeorm/node_modules:/home/pejmanhaghighatnia/Documents/AGI/node_modules/.pnpm/typeorm@0.3.25_pg@8.16.3_redis@4.7.1_reflect-metadata@0.1.14/node_modules:/home/pejmanhaghighatnia/Documents/AGI/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/pejmanhaghighatnia/Documents/AGI/node_modules/.pnpm/typeorm@0.3.25_pg@8.16.3_redis@4.7.1_reflect-metadata@0.1.14/node_modules/typeorm/node_modules:/home/pejmanhaghighatnia/Documents/AGI/node_modules/.pnpm/typeorm@0.3.25_pg@8.16.3_redis@4.7.1_reflect-metadata@0.1.14/node_modules:/home/pejmanhaghighatnia/Documents/AGI/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/typeorm@0.3.25_pg@8.16.3_redis@4.7.1_reflect-metadata@0.1.14/node_modules/typeorm/cli-ts-node-commonjs.js" "$@"
else
  exec node  "$basedir/../.pnpm/typeorm@0.3.25_pg@8.16.3_redis@4.7.1_reflect-metadata@0.1.14/node_modules/typeorm/cli-ts-node-commonjs.js" "$@"
fi
