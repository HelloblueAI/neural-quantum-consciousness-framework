{
  "name": "agi-superintelligence",
  "version": "1.0.0",
  "description": "Next-generation Artificial General Intelligence system with advanced reasoning, learning, and autonomous capabilities",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && tsc-alias",
    "build:core": "tsc src/core/AGISystem.ts src/core/NeuralCore.ts src/core/TrueAGIEngine.ts src/core/NeuralFoundationEngine.ts src/core/learning/UnifiedLearningEngine.ts src/core/reasoning/CrossDomainReasoningEngine.ts src/core/KnowledgeBase.ts src/core/MemoryManager.ts src/core/PerformanceMonitor.ts src/core/SecurityManager.ts src/utils/Logger.ts src/types/index.ts --outDir dist --target es2022 --module esnext --moduleResolution node --allowSyntheticDefaultImports --esModuleInterop",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:unit": "vitest tests/unit",
    "test:integration": "vitest tests/integration",
    "test:e2e": "vitest tests/e2e",
    "test:all": "tsx scripts/test-runner.ts",
    "test:agi": "tsx src/demo/AGIDemonstration.ts",
    "test:advanced-agi": "tsx src/demo/AdvancedAGIDemonstration.ts",
    "test:enhanced-agi": "tsx src/demo/EnhancedAGIDemonstration.ts",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:ci": "vitest --run --coverage --reporter=verbose",
    "lint": "eslint src --ext .ts || echo 'Linting not configured yet'",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.cache",
    "benchmark": "tsx src/benchmarks/performance.ts",
    "deploy": "pnpm run build && pnpm run test:all && echo 'ðŸš€ AGI System deployment ready!'",
    "test:flawless": "pnpm run test:all && echo 'ðŸŒŸ AGI System is FLAWLESS!'",
    "api": "tsx src/api-server.ts",
    "api:prod": "node dist/api-server.js",
    "enhanced-agi": "tsx src/enhanced-api-server.ts",
    "enhanced-agi:dev": "tsx watch src/enhanced-api-server.ts",
    "enhanced-agi:build": "tsc src/enhanced-api-server.ts --outDir dist --module esnext --target es2020",
    "enhanced-agi:start": "node dist/enhanced-agi-api-server.js",
    "enhanced-agi:prod": "node dist/enhanced-agi-api-server.js",
    "neuralcore": "tsx src/neuralcore-api.ts",
    "neuralcore:prod": "node dist/neuralcore-api.js",
    "sentientcore": "tsx src/sentient-web-interface.ts",
    "sentientcore:api": "tsx src/sentient-api.ts",
    "sentientcore:deploy": "tsx deploy/sentient-deployment.js",
    "deploy:heroku": "bash deploy/deploy-heroku.sh",
    "deploy:docker": "bash deploy/deploy-docker.sh",
    "deploy:cloud": "tsx deploy/cloud-deployment.js",
    "worker:dev": "wrangler dev --config wrangler.toml",
    "worker:dev:enhanced": "wrangler dev --config wrangler.enhanced-agi.toml",
    "worker:dev:advanced": "wrangler dev --config wrangler.advanced-agi.toml",
    "worker:dev:real": "wrangler dev --config wrangler.real-agi.toml",
    "worker:dev:test": "wrangler dev --config wrangler.test.toml",
    "deploy:worker": "wrangler deploy --config wrangler.toml",
    "deploy:worker:prod": "wrangler deploy --config wrangler.toml --env production",
    "deploy:worker:staging": "wrangler deploy --config wrangler.toml --env staging",
    "deploy:enhanced": "wrangler deploy --config wrangler.enhanced-agi.toml",
    "deploy:enhanced:prod": "wrangler deploy --config wrangler.enhanced-agi.toml --env production",
    "deploy:advanced": "wrangler deploy --config wrangler.advanced-agi.toml",
    "deploy:advanced:prod": "wrangler deploy --config wrangler.advanced-agi.toml --env production",
    "deploy:real": "wrangler deploy --config wrangler.real-agi.toml",
    "deploy:real:prod": "wrangler deploy --config wrangler.real-agi.toml --env production",
    "deploy:test": "wrangler deploy --config wrangler.test.toml --env test",
    "worker:tail": "wrangler tail --config wrangler.toml",
    "worker:tail:enhanced": "wrangler tail --config wrangler.enhanced-agi.toml",
    "worker:tail:advanced": "wrangler tail --config wrangler.advanced-agi.toml"
  },
  "keywords": [
    "agi",
    "artificial-intelligence",
    "machine-learning",
    "neural-networks",
    "autonomous-systems"
  ],
  "author": "AGI Research Team",
  "license": "MIT",
  "packageManager": "pnpm@10.8.1",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.7.0",
    "@tensorflow/tfjs-node": "^4.17.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.0",
    "express-rate-limit": "^7.5.1",
    "fp-ts": "^2.16.11",
    "helmet": "^7.2.0",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "onnxruntime-node": "^1.23.0",
    "openai": "^4.104.0",
    "pg": "^8.11.0",
    "ramda": "^0.29.1",
    "rate-limiter-flexible": "^3.0.0",
    "redis": "^4.6.0",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.0",
    "supertest": "^6.3.4",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.27",
    "uuid": "^9.0.1",
    "winston": "^3.18.3",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.0",
    "@types/node": "^20.10.0",
    "@types/supertest": "^2.0.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "ffi-napi": "^4.0.3",
    "nodemon": "^3.0.0",
    "prettier": "^3.1.0",
    "ref-napi": "^3.0.3",
    "tsc-alias": "^1.8.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^1.0.0",
    "wrangler": "^4.53.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
