{
  "name": "agi-superintelligence",
  "version": "1.0.0",
  "description": "Next-generation Artificial General Intelligence system with advanced reasoning, learning, and autonomous capabilities",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:unit": "vitest tests/unit",
    "test:integration": "vitest tests/integration",
    "test:e2e": "vitest tests/e2e",
    "test:all": "tsx scripts/test-runner.ts",
    "test:agi": "tsx src/demo/AGIDemonstration.ts",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:ci": "vitest --run --coverage --reporter=verbose",
    "lint": "eslint src --ext .ts || echo 'Linting not configured yet'",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.cache",
    "benchmark": "tsx src/benchmarks/performance.ts",
    "deploy": "npm run build && npm run test:all && echo 'ðŸš€ AGI System deployment ready!'",
    "test:flawless": "npm run test:all && echo 'ðŸŒŸ AGI System is FLAWLESS!'",
    "api": "tsx src/api-server.ts",
    "api:prod": "node dist/api-server.js",
    "neuralcore": "tsx src/neuralcore-api.ts",
    "neuralcore:prod": "node dist/neuralcore-api.js",
    "sentientcore": "tsx src/sentient-web-interface.ts",
    "sentientcore:api": "tsx src/sentient-api.ts",
    "sentientcore:deploy": "tsx deploy/sentient-deployment.js",
    "deploy:heroku": "bash deploy/deploy-heroku.sh",
    "deploy:docker": "bash deploy/deploy-docker.sh",
    "deploy:cloud": "tsx deploy/cloud-deployment.js"
  },
  "keywords": ["agi", "artificial-intelligence", "machine-learning", "neural-networks", "autonomous-systems"],
  "author": "AGI Research Team",
  "license": "MIT",
  "packageManager": "pnpm@10.8.1",
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.17.0",
    "onnxruntime-node": "^1.17.0",

    "openai": "^4.20.0",
    "@anthropic-ai/sdk": "^0.7.0",
    "zod": "^3.22.0",
    "rxjs": "^7.8.0",
    "fp-ts": "^2.16.0",
    "ramda": "^0.29.0",
    "lodash": "^4.17.21",
    "uuid": "^9.0.0",
    "winston": "^3.11.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "rate-limiter-flexible": "^3.0.0",
    "express-rate-limit": "^7.1.0",
    "redis": "^4.6.0",
    "pg": "^8.11.0",
    "typeorm": "^0.3.17",
    "reflect-metadata": "^0.1.13",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "swagger-ui-express": "^5.0.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/compression": "^1.7.0",
    "@types/lodash": "^4.14.0",
    "@types/uuid": "^9.0.0",
    "@types/jest": "^29.5.0",
    "@types/supertest": "^2.0.0",
    "typescript": "^5.3.0",
    "tsx": "^4.6.0",
    "tsc-alias": "^1.8.0",
    "vitest": "^1.0.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "prettier": "^3.1.0",
    "nodemon": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
